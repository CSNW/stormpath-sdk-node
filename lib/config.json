{
  "client": {
    "apiKey": {
      "file": null,
      "id": null,
      "secret": null
    },
    "cacheManager": {
      "defaultTtl": 300,
      "defaultTti": 300,
      "caches": {
        "account": {
          "ttl": 300,
          "tti": 300
        }
      }
    },
    "baseUrl": "https://api.stormpath.com/v1",
    "connectionTimeout": 30,
    "authenticationScheme": "SAUTHC1",
    "proxy": {
      "port": null,
      "host": null,
      "username": null,
      "password": null
    }
  },
  "application": {
    "name": null,
    "href": null
  },
  "web": {
    "oauth2":{
      "enabled": false,
      "uri": "/oauth/token",
      "password": {
        "enabled": true,
        "accessToken": {
          "ttl": 3600
        }
      },
      "client_credentials": {
        "enabled": true,
        "accessToken": {
          "ttl": 3600
        }
      }
    },
    "accessTokenCookie":{
      "name": "access_token",
      "ttl": 3600,
      "tti": 900,
      "httpOnly": true,
      "secure": null,
      "path": "/",
      "domain": null
    },
    "register": {
      "enabled": false,
      "uri": "/register",
      "nextUri": "/",
      "autoAuthorize": false,
      "fields": [
        {
          "name": "givenName",
          "required": true
        },
        {
          "name": "surname",
          "required": true
        },
        {
          "name": "email",
          "required": true,
          "type": "email"
        },
        {
          "name": "password",
          "required": true,
          "type": "password"
        }
      ],
      "view": "register"
    },
    "verifyEmail": {
      "enabled": false,
      "autoLogin": true,
      "uri": "/verify",
      "nextUri": "/",
      "view": "verify"
    },
    "login": {
      "enabled": false,
      "autoLogin": true,
      "uri": "/login",
      "nextUri": "/",
      "view": "login"
    },
    "logout": {
      "enabled": false,
      "uri": "/logout",
      "nextUri": "/"
    },
    "forgotPassword": {
      "enabled": false,
      "uri": "/forgot",
      "view": "forgot",
      "nextUri": "/login?status=forgot"
    },
    "changePassword": {
      "enabled": false,
      "autoLogin": false,
      "uri": "/reset",
      "nextUri": "/login?status=reset",
      "view": "reset",
      "errorUri": "/forgot?status=invalid_sptoken"
    },
    "idSite": {
      "enabled": false,
      "uri": "/idSiteResult",
      "loginUri": "",
      "forgotUri": "/#/forgot",
      "registerUri": "/#/register"
    }
  }
}
